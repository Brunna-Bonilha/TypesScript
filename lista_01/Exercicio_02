const leia = require("readline-sync");

const num: number = leia.questionInt("\nDigite o numero: ");
const arr: number[] = [2, 5, 1, 3, 4, 9, 7, 8, 10, 6];


if(arr.includes(num)){
    const position: number = arr.indexOf(num);
    console.log(`\nO número ${num} está localizado na posição: ${position}`);
} else{
    console.log(`\nO número ${num} não foi localizado!`);
}